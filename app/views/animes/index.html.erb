<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <h2> Anime List </h2>
      <% @animes.each do |anime| %>
        Anime Name: <%= anime.title %><br/>
        Summary: <%= anime.information %><br/>
        Image: <%= image_tag anime.image %>
      <% end %>
    </div>
  </div>
</div>